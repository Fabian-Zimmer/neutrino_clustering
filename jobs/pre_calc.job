#!/bin/bash
#SBATCH --job-name=PreCalc
#SBATCH -p thin
#SBATCH -t 01:00:00
#SBATCH -N 1
#SBATCH --tasks-per-node=1
#SBATCH --cpus-per-task=1
#SBATCH -o jobs/pre_calc.out
#SBATCH -e jobs/pre_calc.err
#SBATCH -v
#SBATCH --exclusive

module purge
source $HOME/.conda/envs/neutrino_clustering/bin/activate

# Run precalculations.
python $HOME/neutrino_clustering_V2/discrete_precalculations_MW.py